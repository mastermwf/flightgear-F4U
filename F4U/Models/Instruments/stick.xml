<?xml version="1.0"?>

<PropertyList>

<path>stick.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Stick</object-name>
  <property>controls/flight/elevator</property>
  <factor>-10</factor>
  <center>
   <x-m>0.00</x-m>
   <y-m>0.00</y-m>
   <z-m>0.00</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
 <animation>
  <type>translate</type>
  <object-name>Trigger</object-name>
  <property>controls/armament/trigger</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>-0.01</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>Trigger</object-name>
  <property>controls/flight/elevator</property>
  <factor>-10</factor>
  <center>
   <x-m>0.00</x-m>
   <y-m>0.00</y-m>
   <z-m>0.00</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>Trigger</object-name>
  <property>controls/flight/aileron</property>
  <factor>-10</factor>
  <center>
   <x-m>0.00</x-m>
   <y-m>0.00</y-m>
   <z-m>0.00</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Stick</object-name>
  <property>controls/flight/aileron</property>
  <factor>-10</factor>
  <center>
   <x-m>0.00</x-m>
   <y-m>0.00</y-m>
   <z-m>0.00</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
</PropertyList>
