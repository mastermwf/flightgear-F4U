<?xml version="1.0"?>

<!-- flap handle -->

<PropertyList>

 <animation>
  <type>material</type>
   <object-name>frame</object-name>
   <object-name>lever</object-name>
   <object-name>scale</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

	<path>flap-sw.ac</path>
	
	
<animation>
	<type>rotate</type>
		<object-name>lever</object-name>
		<property>/controls/flight/flaps</property>
		<factor>45</factor>
		<offset-deg>0</offset-deg>
		<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
		</center>
		<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
		</axis>		
</animation>
	


		
</PropertyList>
